{
  "extends": ["tslint:recommended", "tslint-consistent-codestyle", "tslint-config-prettier", "tslint-plugin-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "prettier": true,
    "no-var-requires": {"severity": "warn"},
    "interface-name": false,
    "no-console": false,
    "no-empty-interface": false,
    "member-access": false
  },
  "linterOptions": {
    "format": "verbose"
  },
  "format": "verbose"
}
